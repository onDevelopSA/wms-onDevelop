<odoo>
  <record id="form_stock_warehouse_inh" model="ir.ui.view">
    <field name="name">form.stock.warehouse.inh</field>
    <field name="model">stock.warehouse</field>
    <field eval="12" name="priority"/>
    <field name="inherit_id" ref="stock.view_warehouse"/>
    <field name="arch" type="xml">
      <xpath expr="//sheet/notebook" position="inside">
        <page string="Active Employee" id='active_employee_page' colspan="4">
	  <group colspan="4">
	    <field name="active_employee_ids" nolabel="1">
	      <tree name="Active Users" editable="top"
		    colors="red:is_active=='true';">
		<field name="employee_id" string="Employee"
		       domain="[('is_company', '=', False)]"/>
		<field name="zone_id" string="Zone"/>
		<field name="is_active" string="Active"/>
              </tree>
	    </field>	
	  </group>
	</page>
      </xpath>
    </field>
  </record>
</odoo>
